linearlayout mainLayout fill {
	orientation : "vertical"
	textview help "Click on the button to navigate to the other activity!" wrap
	
	button NavigateToOtherActivity "Click me" fill&wrap {
		onClick navigateTo(AndroTextExamples.OtherActivity) 
	}
		
}

linearlayout otherLayout fill {	
	textview help 
	"Hello there, this is the other activity, navigation was successful! Press BACK to return to main activity!" wrap
}