gamegui MenuExampleGui {
	
	texture menu_reset "menu_reset.png"
	texture menu_quit "menu_quit.png"
	texture boxFace "face_box.png"
	
	menu simpleMenu background color(0.09804 0.6274 0.8784) {
		
		menuitem Reset texture menu_reset 
		menuitem Quit  texture menu_quit
		
		sprite face pos(0 0) boxFace
	}
	
}

gamelogic MenuExampleLogic {
	
	move flyAround 30 from(0 0) to(656 414)
	bind flyAround to MenuExampleGui.simpleMenu.face
	
}